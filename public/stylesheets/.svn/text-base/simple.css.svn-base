/* special */

#signup_button {
	text-align: center;
	margin: 10px auto;
	width: 130px;
	padding: 10px;
}
#signup_button a{
	color: #376AF9;
	font-size: 20px;
	font-weight: 500;
}

a[class ="signup"] {
   padding-right: 18px;
   background: transparent url(/social_network/images/icon_popup.gif) no-repeat center right;
}
/* for all form where margin must be very large */
form.form p{
	margin: 20px 0;
	padding: 10px 0;
}


