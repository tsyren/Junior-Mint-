/* CSS style */


/* templates css
 *
 * 	border: 1px solid #000;
 *
 */


/* html */


body,h1,h2,h3,h4,h5,p,li,ul,input,label,form {
	padding: 0;
	margin: 0;
}
body {
	background-color: #C4D4FB;
}
img {
	border: none;
}
a {
	text-decoration:none;
	color: #688CF1;
}
a:hover {
	color: #688CF1;
}
ul {
	list-style: none;
}

/* ids */


#body {
	width: 800px;
	margin: 0 auto;
}
#header {
	height: 60px;
	margin: 20px 0;
}
#content {

}
#footer {
	margin: 20px 0;

}

/* --------------------------- */
/* Flash message               */

#notice {
	padding: 10px;
	margin: 20px 0;
	background-color: #FBEEAD;
}
#Flash ul{
	padding-left: 20px;
}

/* --------------------------- */
/* Border of flash message    */

/* при указании путя - ставить слэшь url(/images/...)*/
/* пока нет боковых */



/* login form */

#login input{
	border: 1px solid #D5CFCF;
	height: 21px;
	text-align:left;
	font-size: 1em;
	font-weight: 500;
	position: static;
	padding: 3px 0;
}
#login p{
	margin-top: 10px;
}
#remember_me {
	margin-top: 5px;
	font-size: 12px;
}
#remember_me input{
	margin-right: 5px;
    vertical-align: -3px;
	height: 13px;
	width: 13px;
	text-align: center;
	padding: 1px;
}
.helper {
	margin-top: 10px;
}
#button input{
	border: 1px solid #D5CFCF;
	height: 30px;
	text-align:left;
	padding: 5px 10px;
	font-size: 14px;
	font-weight: 500;
	margin-top: 10px;
}
.fieldWithErrors {
	padding: 0;
	margin: 0;
}
input#openid_url {
   background: url(http://openid.net/login-bg.gif) no-repeat;
   background-color: #fff;
   background-position: 0 50%;
   color: #000;
   padding-left: 18px;
}

/* Block */

.block {
  margin: 70px auto;
  padding: 20px;
  width: 300px;
  height: 200px;
}

/* login page */
#go_home {
  float:right;
  margin-top: -20px;
}

/* top positions classes */

.left {
  	float: left;
  	width: 431px;
}
.right {
  	float: right;
  	width: 300px;
}
.right .prompt {
	background-color: #B5FB99;
	padding: 0px;
	margin: 10px 0;
	height: 100%;
 	color: #315DEE;
}

.left, .right {
    padding: 12px;
}
.clear {
	clear: both;
}



